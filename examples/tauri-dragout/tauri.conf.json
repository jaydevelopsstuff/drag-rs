{
  "identifier": "dev.crabnebula.drag",
  "build": {
    "frontendDist": [
      "../index.html",
      "./new_window.html"
    ]
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Drag",
        "width": 300,
        "height": 400,
        "dragDropEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../icon.icns",
      "../icon.ico",
      "../icon.png"
    ]
  }
}
